<% provide(:title, 'All hurdle athletes') %>
<h1> All hurdle athletes</h1>

<table border="3" cellpadding="3">
	<tr bgcolor="yellow">
		<th>Name</th>
		<th>Gender</th>
		<th>Qualification Time</th>
		<th>Round</th>
	</tr>
	<% @hurdles.each do |hurdle| %>
		<tr>
			<td><%= link_to hurdle.first_name + " "+ hurdle.last_name, hurdle %></td>
			<td><%= hurdle.gender %></td>
			<td><%= hurdle.qualification.strftime('%H:%M:%S') %></td>
			<td><%= hurdle.round %></td>
		</tr>
	<% end %>
</table>
