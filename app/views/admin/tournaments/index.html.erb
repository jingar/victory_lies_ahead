<% provide(:title, 'Tournaments') %>
<h1>Tournaments</h1>

<ul class = "tournaments">
  <% @tournaments.each do |tournament| %>
    <li> 
      <%= link_to tournament.name, [:admin,tournament] %>
    </li>
  <% end %>
</ul>
<p><%= link_to "New Tournament", new_admin_tournament_path %></p>
<p><%= link_to "Back to Dashboard", admin_path %></p>
